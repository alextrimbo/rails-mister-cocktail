<div class="container">
<h1>**** Cocktails ****</h1>
<%= link_to 'New Cocktail', new_cocktail_path, class: "btn btn-success" %>
<% @cocktails.each do |cocktail| %>
  <h2> <%= link_to cocktail.name, cocktail_path(cocktail) %></h2>
  <p><%=link_to "New Dose", new_cocktail_dose_path(cocktail), class: "btn btn-primary" %> </p>
  <hr>
<% end %>
</div>
